{
	"compilerOptions":
	{
		"declaration": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"module": "commonjs",
		"noImplicitAny": true,
		"out": "haeckel.js",
		"preserveConstEnums": true,
		"removeComments": false,
		"sourceMap": true,
		"target": "ES6"
	},
	"exclude": [
		"dist",
		"node_modules"
	]
}
